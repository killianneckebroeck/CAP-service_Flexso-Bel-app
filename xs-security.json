{
  "xsappname": "Bel_app",
  "tenant-mode": "dedicated",
  "oauth2-configuration": {
    "grant-types": [
      "password",
      "authorization_code",
      "client_credentials",
      "refresh_token"
    ],
    "redirect-uris": [
      "https://*.us10-001.hana.ondemand.com/**"
    ]
  },
  "allowedCorsOrigins": [
    "http://localhost:8080",
    "https://*.hana.ondemand.com"
  ],
  "allowedHeaders": [
    "Content-Type",
    "Authorization",
    "x-csrf-token"
  ],
  "allowedMethods": [
    "GET",
    "POST",
    "PUT",
    "DELETE",
    "OPTIONS"
  ]
}
